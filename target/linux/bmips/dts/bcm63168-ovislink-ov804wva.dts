#include "bcm63268.dtsi"

/ {
	model = "OvisLink OV804WVA";
	compatible = "ovislink,ov804wva", "brcm,bcm63168", "brcm,bcm63268";

	aliases {
		led-boot = &led_power_green;
		led-failsafe = &led_power_red;
		led-running = &led_power_green;
		led-upgrade = &led_power_green;
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <100>;

		24ghz {
			label = "2.4GHz";
			gpios = <&gpio 33 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WLAN>;
			debounce-interval = <60>;
		};

		5ghz {
			label = "5GHz";
			gpios = <&gpio 34 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RFKILL>;
			debounce-interval = <60>;
		};

		reset {
			label = "reset";
			gpios = <&gpio 32 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
			debounce-interval = <60>;
		};
	};
};

&ehci {
	status = "okay";
};

&ethernet {
	status = "okay";

	nvmem-cells = <&macaddr_cfe_6a0>;
	nvmem-cell-names = "mac-address";
};

&hsspi {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		spi-max-frequency = <16666667>;
		spi-tx-bus-width = <2>;
		spi-rx-bus-width = <2>;
		reg = <0>;

		#address-cells = <1>;
		#size-cells = <1>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "cfe";
				reg = <0x000000 0x010000>;
				read-only;

				nvmem-layout {
					compatible = "fixed-layout";
					#address-cells = <1>;
					#size-cells = <1>;

					macaddr_cfe_6a0: macaddr@6a0 {
						reg = <0x6a0 0x6>;
					};
				};
			};

			partition@0x010000 {
				compatible = "brcm,bcm963xx-imagetag";
				label = "firmware";
				reg = <0x010000 0xfd0000>;
			};

			partition@fe0000 {
				label = "nvram";
				reg = <0xfd0000 0x030000>;
			};
		};
	};
};

&leds {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_leds>;

	usb1_green@8 {
		reg = <8>;
		active-low;
		label = "green:usb1";
		function = LED_FUNCTION_USB;
		color = <LED_COLOR_ID_GREEN>;
	};

	internet_green@9 {
		reg = <9>;
		active-low;
		label = "green:internet";
	};

	usb2_green@14 {
		reg = <14>;
		active-low;
		label = "green:usb2";
	};

	inet_red@18 {
		reg = <18>;
		active-low;
		label = "red:inet";
	};

	dsl_green@19 {
		reg = <19>;
		active-low;
		label = "green:dsl";
	};

	led_power_green: power_green@20 {
		reg = <20>;
		active-low;
		label = "green:power";
		function = LED_FUNCTION_POWER;
		color = <LED_COLOR_ID_GREEN>;
	};

	led_power_red: power_red@21 {
		reg = <21>;
		active-low;
		label = "red:power";
		function = LED_FUNCTION_POWER;
		color = <LED_COLOR_ID_RED>;
		panic-indicator;
	};

	wps_green@22 {
		reg = <22>;
		active-low;
		label = "green:wps";
	};

	wan_green@23 {
		reg = <23>;
		active-low;
		label = "green:wan";
	};
};

&mdio_ext {
	switch@1e {
		compatible = "brcm,bcm53125";
		reg = <30>;

		dsa,member = <1 0>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@1 {
				reg = <1>;
				label = "lan1";
			};

			port@2 {
				reg = <2>;
				label = "lan2";
			};

			port@3 {
				reg = <3>;
				label = "lan3";
			};

			port@4 {
				reg = <4>;
				label = "lan4";
			};

			port@8 {
				reg = <8>;

				phy-mode = "rgmii";
				ethernet = <&switch0port4>;

				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};
		};
	};
};

&ohci {
	status = "okay";
};

&pcie {
	status = "okay";
};

&pinctrl {
	pinctrl_leds: leds {
		function = "led";
		pins = "gpio8", "gpio9", 
			"gpio14", "gpio18",
			"gpio19", "gpio20",
			"gpio21", "gpio22",
			"gpio23";
			
	};
};

&switch0 {
	dsa,member = <0 0>;

	ports {
		port@3 {
			reg = <3>;
			label = "wan";

			phy-handle = <&phy4>;
			phy-mode = "gmii";
		};

		switch0port4: port@4 {
			reg = <4>;
			label = "extsw";

			phy-mode = "rgmii";

			fixed-link {
				speed = <1000>;
				full-duplex;
			};
		};
	};
};

&uart0 {
	status = "okay";
};

&usbh {
	status = "okay";
};
